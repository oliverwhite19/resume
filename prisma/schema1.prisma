generator client {
  provider = "prisma-client-js"
  output   = "./generated/client1"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id             String      @id @default(uuid()) @db.Uuid
  nickname       String?
  name           String?
  picture        String?
  givenName      String?
  familyName     String?
  email          String   @unique
  role           String   @default("USER")
  createdAt      DateTime @default(now())
  updatedAt      DateTime @default(now())
}
