{
    "name": "comet-land",
    "version": "0.0.1",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev --parallel",
        "lint": "turbo run lint",
        "lint:fix": "turbo run lint:fix",
        "format": "prettier --write \"**/*.{ts,tsx,md}\""
    },
    "engines": {
        "npm": ">=7.0.0",
        "node": ">=14.0.0"
    },
    "packageManager": "yarn@1.22.17",
    "devDependencies": {
        "@types/node": "^17.0.12",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "codecov": "^3.8.3",
        "esbuild": "^0.14.31",
        "esbuild-jest": "^0.5.0",
        "eslint": "^7.32.0",
        "identity-obj-proxy": "^3.0.0",
        "prettier": "^2.5.1",
        "ts-jest": "^28.0.3",
        "turbo": "^1.1.10",
        "typescript": "^4.5.3"
    }
}
